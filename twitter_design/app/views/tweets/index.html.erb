
<h3>Tweets</h3>
  <% @tweets.each do |tweet| %>
      <div class="well well-small">
          <dl class="dl-horizontal">
            <dt><%= image_tag("#{tweet["profile_image_url"]}")%></dt>
            <dd><strong><i class="icon-user"><%= tweet["from_user_name"] %></i></strong>
              <dd><%= link_to tweet["text"], tweet_path(tweet) %></dd>
          </dl>
      </div>
  <% end %>